<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.thenounproject" ExtensionBundleName="Noun Project" ExtensionBundleVersion="1.0.0" Version="4.0">
	<ExtensionList>
		<Extension Id="com.thenounproject.nounPanel" Version="1.0"/>
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="ILST" Version="17.0" />
			<Host Name="PHSP" Version="14.0"/>
			<Host Name="PHXS" Version="14.0"/>
			<Host Name="IDSN" Version="11.0" />
		</HostList>
		<LocaleList>
			<Locale Code="All"/>
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="4.0"/>
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.thenounproject.nounPanel">
			<DispatchInfo>
				<Resources>
					<MainPath>./index.html</MainPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>Noun Project</Menu>
					<Geometry>
						<Size>
							<Height>700</Height>
							<Width>405</Width>
						</Size>
						<MaxSize>
							<Height>1200</Height>
							<Width>1200</Width>
						</MaxSize>
						<MinSize>
							<Height>580</Height>
							<Width>350</Width>
						</MinSize>
					</Geometry>
					<Icons>
						<Icon Type="Normal">./img/iconNormal.png</Icon>
						<Icon Type="RollOver">./img/iconRollover.png</Icon>
						<Icon Type="DarkNormal">./img/IconDarkNormal.png</Icon>
						<Icon Type="DarkRollOver">./img/IconDarkRollover.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
